{
    "manifest_version": 3,
    "name": "IP Address Opener",
    "version": "1.0",
    "description": "Open IP addresses in the browser.",
    "action": {
        "default_popup": "/content/ip_opener.html"

    },
    "permissions": [ "storage", "tabs", "contextMenus", "activeTab", "scripting", "clipboardRead", "clipboardWrite"],
    "background": {
        "service_worker": "./background/background.js"
    }

}